{
  "name": "lol-teleport",
  "description": "Easily jump between the same player on different League of Legends stats websites. Supports OP.GG, U.GG, Deeplol, and more.",
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "wxt prepare",
    "test": "vitest",
    "test:run": "vitest run",
    "format:write": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.19.0",
    "@tsconfig/svelte": "^5.0.4",
    "@types/chrome": "^0.0.301",
    "@types/node": "^22.13.1",
    "@wxt-dev/module-svelte": "^2.0.0",
    "autoprefixer": "10.4.20",
    "bits-ui": "^1.0.0-next.84",
    "clsx": "^2.1.1",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "lucide-svelte": "^0.474.0",
    "postcss": "8.4.47",
    "prettier": "3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.1.16",
    "svelte-check": "^4.0.7",
    "tailwind-merge": "^3.0.1",
    "tailwind-variants": "^0.3.1",
    "tailwindcss": "3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.23.0",
    "typescript-svelte-plugin": "^0.3.45",
    "vitest": "^3.0.5",
    "wxt": "^0.19.13"
  },
  "dependencies": {
    "mode-watcher": "^0.5.1",
    "posthog-js": "^1.215.5"
  }
}
